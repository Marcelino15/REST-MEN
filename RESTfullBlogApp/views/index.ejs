<%- include("partials/header") %>
<h1>INDEX PAGE!!!</h1>
<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" alt="images">
        <span><%= blog.created.toDateString() %></span>
        <p><%- blog.body.substring(0,100) %>...</p>
        <a href="/blogs/<%= blog.id %>">Read More</a>
    </div>
<% })%>
<%- include("partials/footer") %>